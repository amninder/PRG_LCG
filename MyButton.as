package  {	import flash.display.Sprite;		public class MyButton extends Sprite {				public function MyButton() {					}				public function createButton(lambai:Number, chaurai:Number, text:String, borderColor:uint, bodyColor:uint):Sprite{			var rect:Sprite = new Sprite();			rect.graphics.lineStyle(3, borderColor);			rect.graphics.beginFill(borderColor);			//rect.graphics.drawRect(0,0, lambai, chaurai);			rect.graphics.drawRoundRect(0,0,lambai,chaurai,10,10);			rect.graphics.endFill();						addChild(rect);			return rect;		}	}	}